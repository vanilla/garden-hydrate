{
    "name": "vanilla/garden-hydrate",
    "description": "A JSON based templating language for data.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Charron",
            "email": "adam@charrondev.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "vanilla/garden-jsont": "^1.2",
        "vanilla/garden-schema": "^1.10.2",
        "symfony/cache": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Garden\\Hydrate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garden\\Hydrate\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.0",
        "symfony/polyfill-mbstring": "1.18.1"
    }
}
